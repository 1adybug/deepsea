            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "dev": "rslib build --watch",
        "build": "rslib build",
        "prebuild": "tsx scripts/export.ts",
        "prepublishOnly": "npm run build",
        "lint": "prettier --write ."
    },
    "keywords": [],
    "authors": [
        "lurongv@qq.com"
    ],
    "license": "MIT",
    "files": [
        "compiled",
