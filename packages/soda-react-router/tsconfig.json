{
    "compilerOptions": {
        "strict": true,
        "declaration": true,
        "skipLibCheck": true,
        "baseUrl": "./",
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "paths": {
            "@hooks/*": [
                "./src/hooks/*"
            ],
            "@utils/*": [
                "./src/utils/*"
            ]
        }
    }
}